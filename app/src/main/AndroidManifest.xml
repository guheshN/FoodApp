<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.np.foodapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.np.foodapp.StartingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.np.foodapp.FoodStallReview" />
        <activity android:name="com.np.foodapp.SorryNotAvailablePage" />
        <activity android:name="com.np.foodapp.StallRV" />
        <activity android:name="com.np.foodapp.CourtRV" />
        <activity android:name="com.np.foodapp.PostReview" />
        <activity android:name="com.np.foodapp.RegisterActivity" />
        <activity android:name="com.np.foodapp.LoginActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>